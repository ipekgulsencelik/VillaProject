@model CreateContactDTO

@{
    ViewData["Title"] = "Yeni İletişim Bilgisi Ekleme Sayfası";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<form method="post">
    <label>Harita URL</label>
    <input type="text" asp-for="MapURL" class="form-control" />
    <br />
    <label>Telefon</label>
    <input type="tel" asp-for="Phone" class="form-control" />
    <br />
    <label>Email</label>
    <input type="email" asp-for="Email" class="form-control" />
    <br />
    <button class="btn btn-outline-dark"><i class="la la-save"></i> Kaydet</button>
</form>