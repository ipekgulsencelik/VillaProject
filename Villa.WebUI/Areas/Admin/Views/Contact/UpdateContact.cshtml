@model UpdateContactDTO

@{
    ViewData["Title"] = "UpdateContact";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<<form method="post">
    <label>ID</label>
    <input type="text" asp-for="ID" disabled class="form-control" />
    <br />
    <label>Harita URL</label>
    <input type="text" asp-for="MapURL" class="form-control" />
    <br />
    <label>Telefon</label>
    <input type="tel" asp-for="Phone" class="form-control" />
    <br />
    <label>Email</label>
    <input type="email" asp-for="Email" class="form-control" />
    <br />
    <button class="btn btn-outline-dark"><i class="la la-save"></i> Güncelle</button>
</form>